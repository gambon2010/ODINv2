digraph structs {
  ranksep=1;
  /* node [shape=box]; */

  {
    node [shape=plaintext, fontsize=16];
    Input -> I -> II -> III -> IV -> V -> VI;
  }

  { rank = same;
    Input;
    properties; globalStyle; layerStyle; featureStyle; mode;
    geometryKey; centerResolution;
  };

  { rank = same;
    I;
    node [shape=record];
    PROPERTIES [label="{{<sidc> sidc|<parameterizedSIDC> parameterized\nSIDC|<modifiers> modifiers}}"];
  };

  { rank = same;
    II;
    node [shape=record];
    EFFECTIVE [label="{{<smoothen> smoothen|<effectiveStyle> effectiveStyle}}"];

    node [shape=box];
    evalSync;

    node [shape=record];
    STYLES [label="{{<dynamicStyle> dynamicStyle|<staticStyles> staticStyles}}"];
  };

  { rank = same;
    III;
    node [shape=record];
    GEOMETRY [label="{{<simplified> simplified|<resolution> resolution|<geometry> geometry|<rewrite> rewrite}}"];
  };

  { rank = same;
    IV;
    node [shape=box];
    placement;
  };

  { rank = same;
    V;
    node [shape=box];
    styles;
  };

  { rank = same;
    VI;
    node [shape=box];
    style;
  };

  properties -> PROPERTIES;
  PROPERTIES:sidc:s -> EFFECTIVE:effectiveStyle:n;
  globalStyle -> EFFECTIVE;
  layerStyle -> EFFECTIVE;
  featureStyle -> EFFECTIVE;
  PROPERTIES:parameterizedSIDC -> STYLES:n;
  PROPERTIES:modifiers -> evalSync;
  mode -> GEOMETRY;
  EFFECTIVE:smoothen:s -> GEOMETRY;
  geometryKey -> GEOMETRY;
  centerResolution -> GEOMETRY;
  GEOMETRY:geometry:s -> placement:n;
  placement -> styles;
  evalSync:s -> styles;
  STYLES -> styles;
  GEOMETRY:rewrite:s -> style [style=dashed];
  styles -> style;
  EFFECTIVE:effectiveStyle:s -> style;
}
